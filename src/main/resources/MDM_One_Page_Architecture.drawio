<mxfile host="app.diagrams.net" modified="2025-08-22T15:04:50Z" agent="python-script" version="22.0.3" editor="draw.io"><diagram name="MDM One-Page Architecture"><mxGraphModel dx="1280" dy="720" grid="1" gridSize="10" guides="1" tooltips="1" connect="1" arrows="1" fold="1" page="1" pageScale="1" pageWidth="2200" pageHeight="1400" math="0" shadow="0"><root><mxCell id="0" /><mxCell id="1" parent="0" /><mxCell id="3" value="&lt;div style='font-size:14px'&gt;&lt;b&gt;MDM One-Page Architecture&lt;/b&gt;&lt;div style='font-size:12px;margin-top:4px'&gt;Single staging ➜ deterministic &amp; fuzzy dedup ➜ Single Source of Truth&lt;/div&gt;&lt;/div&gt;" style="rounded=1;whiteSpace=wrap;html=1;shadow=0;fillColor=#eef2ff;strokeColor=#6366f1;fontColor=#1e293b;spacing=8;fontStyle=1;" vertex="1" parent="1"><mxGeometry x="20" y="20" width="1760" height="60" as="geometry" /></mxCell><mxCell id="4" value="&lt;div style='font-size:14px'&gt;&lt;b&gt;Core fuzzy across all&lt;/b&gt;&lt;div style='font-size:12px;margin-top:4px'&gt;• First Name • Last Name • DOB&lt;br/&gt;• Gender (filter)&lt;/div&gt;&lt;/div&gt;" style="rounded=1;whiteSpace=wrap;html=1;shadow=0;fillColor=#f8fafc;strokeColor=#cbd5e1;fontColor=#334155;spacing=8;" vertex="1" parent="1"><mxGeometry x="20" y="95" width="540" height="80" as="geometry" /></mxCell><mxCell id="5" value="&lt;div style='font-size:14px'&gt;&lt;b&gt;Opportunistic boosts&lt;/b&gt;&lt;div style='font-size:12px;margin-top:4px'&gt;• Middle Name (PRS/DUES)&lt;br/&gt;• Email &amp; Phone (MYQUEST)&lt;br/&gt;• Address (MYQUEST/DUES)&lt;/div&gt;&lt;/div&gt;" style="rounded=1;whiteSpace=wrap;html=1;shadow=0;fillColor=#f8fafc;strokeColor=#cbd5e1;fontColor=#334155;spacing=8;" vertex="1" parent="1"><mxGeometry x="580" y="95" width="560" height="80" as="geometry" /></mxCell><mxCell id="6" value="&lt;div style='font-size:14px'&gt;&lt;b&gt;Deterministic joins&lt;/b&gt;&lt;div style='font-size:12px;margin-top:4px'&gt;• PRS_PROFILE_ID (PRS↔MYQUEST)&lt;br/&gt;• PATIENT_ID (MYQUEST↔DUES)&lt;br/&gt;• EID / EMPI / patient_eid (when present)&lt;/div&gt;&lt;/div&gt;" style="rounded=1;whiteSpace=wrap;html=1;shadow=0;fillColor=#f8fafc;strokeColor=#cbd5e1;fontColor=#334155;spacing=8;" vertex="1" parent="1"><mxGeometry x="1150" y="95" width="630" height="80" as="geometry" /></mxCell><mxCell id="7" value="&lt;div style='font-size:14px'&gt;&lt;b&gt;Sources&lt;/b&gt;&lt;div style='font-size:12px;margin-top:4px'&gt;PRS (Oracle) – PATIENT_PROFILES&lt;br/&gt;MYQUEST (Oracle) – MAIN_PATIENT_PROFILES&lt;br/&gt;ECPM (MySQL) – DUES_PERSONS&lt;/div&gt;&lt;/div&gt;" style="rounded=1;whiteSpace=wrap;html=1;shadow=0;fillColor=#ffffff;strokeColor=#94a3b8;fontColor=#0f172a;spacing=8;" vertex="1" parent="1"><mxGeometry x="20" y="210" width="260" height="190" as="geometry" /></mxCell><mxCell id="8" value="&lt;div style='font-size:14px'&gt;&lt;b&gt;Ingest &amp; Standardize&lt;/b&gt;&lt;div style='font-size:12px;margin-top:4px'&gt;CDC/ELT (GoldenGate/Debezium/DMS)&lt;br/&gt;Normalize: case/trim/date&lt;br/&gt;Phone→E.164, Email validate&lt;br/&gt;Address tokenize (house#, street)&lt;/div&gt;&lt;/div&gt;" style="rounded=1;whiteSpace=wrap;html=1;shadow=0;fillColor=#ffffff;strokeColor=#94a3b8;fontColor=#0f172a;spacing=8;" vertex="1" parent="1"><mxGeometry x="320" y="210" width="260" height="190" as="geometry" /></mxCell><mxCell id="9" value="&lt;div style='font-size:14px'&gt;&lt;b&gt;Unified Staging&lt;/b&gt;&lt;div style='font-size:12px;margin-top:4px'&gt;&lt;b&gt;STG_PERSON&lt;/b&gt;&lt;br/&gt;first/middle/last, dob, gender&lt;br/&gt;email, phone, address&lt;br/&gt;created/modified&lt;br/&gt;&lt;hr/&gt;&lt;b&gt;STG_PERSON_KEYS&lt;/b&gt;&lt;br/&gt;PRS_PROFILE_ID, PATIENT_ID&lt;br/&gt;EMAIL, PHONE7, NAMEZIP, STREETNAME&lt;/div&gt;&lt;/div&gt;" style="rounded=1;whiteSpace=wrap;html=1;shadow=0;fillColor=#ffffff;strokeColor=#94a3b8;fontColor=#0f172a;spacing=8;" vertex="1" parent="1"><mxGeometry x="620" y="210" width="260" height="190" as="geometry" /></mxCell><mxCell id="10" value="&lt;div style='font-size:14px'&gt;&lt;b&gt;Matching Engine&lt;/b&gt;&lt;div style='font-size:12px;margin-top:4px'&gt;&lt;b&gt;Deterministic&lt;/b&gt;: PRS_PROFILE_ID, PATIENT_ID&lt;br/&gt;&lt;b&gt;Blocking&lt;/b&gt;: EMAIL, PHONE7, NAMEZIP, STREETNAME&lt;br/&gt;&lt;b&gt;Fuzzy (dynamic weights)&lt;/b&gt;:&lt;br/&gt;• Names (+middle) • DOB (exact/±1)&lt;br/&gt;• Address • Email/Phone when present&lt;/div&gt;&lt;/div&gt;" style="rounded=1;whiteSpace=wrap;html=1;shadow=0;fillColor=#ffffff;strokeColor=#94a3b8;fontColor=#0f172a;spacing=8;" vertex="1" parent="1"><mxGeometry x="920" y="210" width="260" height="190" as="geometry" /></mxCell><mxCell id="11" value="&lt;div style='font-size:14px'&gt;&lt;b&gt;Cluster &amp; Survivorship&lt;/b&gt;&lt;div style='font-size:12px;margin-top:4px'&gt;Connected components (keys)&lt;br/&gt;Score ≥ 0.90 → auto-link&lt;br/&gt;&lt;b&gt;Selection&lt;/b&gt;:&lt;br/&gt;1) Created Date desc&lt;br/&gt;2) Verified Email&lt;br/&gt;3) Has Email&lt;br/&gt;4) Source Priority (MYQUEST &gt; PRS &gt; ECPM)&lt;/div&gt;&lt;/div&gt;" style="rounded=1;whiteSpace=wrap;html=1;shadow=0;fillColor=#ffffff;strokeColor=#94a3b8;fontColor=#0f172a;spacing=8;" vertex="1" parent="1"><mxGeometry x="1220" y="210" width="260" height="190" as="geometry" /></mxCell><mxCell id="12" value="&lt;div style='font-size:14px'&gt;&lt;b&gt;Outputs&lt;/b&gt;&lt;div style='font-size:12px;margin-top:4px'&gt;&lt;b&gt;SOT_PERSON&lt;/b&gt; (Single Source of Truth)&lt;br/&gt;&lt;b&gt;STG_PERSON_DEDUP&lt;/b&gt; (view)&lt;br/&gt;&lt;b&gt;STG_PERSON_XREF&lt;/b&gt; (lineage)&lt;br/&gt;Optional: REVIEW_Q (0.75–0.89)&lt;/div&gt;&lt;/div&gt;" style="rounded=1;whiteSpace=wrap;html=1;shadow=0;fillColor=#ffffff;strokeColor=#94a3b8;fontColor=#0f172a;spacing=8;" vertex="1" parent="1"><mxGeometry x="1520" y="210" width="260" height="190" as="geometry" /></mxCell><mxCell id="13" value="&lt;div style='font-size:14px'&gt;&lt;b&gt;Governance &amp; Monitoring&lt;/b&gt;&lt;div style='font-size:12px;margin-top:4px'&gt;DQ checks (email/phone/DOB)&lt;br/&gt;Audit merges/splits &amp; lineage&lt;br/&gt;RBAC / masking / encryption&lt;br/&gt;Metrics: precision/recall, cluster sizes&lt;/div&gt;&lt;/div&gt;" style="rounded=1;whiteSpace=wrap;html=1;shadow=0;fillColor=#ffffff;strokeColor=#94a3b8;fontColor=#0f172a;spacing=8;" vertex="1" parent="1"><mxGeometry x="1520" y="440" width="260" height="150" as="geometry" /></mxCell><mxCell id="14" style="endArrow=block;endFill=1;strokeColor=#94a3b8;strokeWidth=2;" edge="1" parent="1" source="7" target="8"><mxGeometry relative="1" as="geometry" /></mxCell><mxCell id="15" style="endArrow=block;endFill=1;strokeColor=#94a3b8;strokeWidth=2;" edge="1" parent="1" source="8" target="9"><mxGeometry relative="1" as="geometry" /></mxCell><mxCell id="16" style="endArrow=block;endFill=1;strokeColor=#94a3b8;strokeWidth=2;" edge="1" parent="1" source="9" target="10"><mxGeometry relative="1" as="geometry" /></mxCell><mxCell id="17" style="endArrow=block;endFill=1;strokeColor=#94a3b8;strokeWidth=2;" edge="1" parent="1" source="10" target="11"><mxGeometry relative="1" as="geometry" /></mxCell><mxCell id="18" style="endArrow=block;endFill=1;strokeColor=#94a3b8;strokeWidth=2;" edge="1" parent="1" source="11" target="12"><mxGeometry relative="1" as="geometry" /></mxCell><mxCell id="19" style="endArrow=block;endFill=1;strokeColor=#94a3b8;strokeWidth=2;" edge="1" parent="1" source="12" target="13"><mxGeometry relative="1" as="geometry" /></mxCell></root></mxGraphModel></diagram></mxfile>